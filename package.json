{
  "name": "dashboard-editor",
  "version": "1.0.0",
  "main": "dist/main/index.cjs",
  "author": "Joel <xiao2731962667@163.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "build": "vue-tsc --project src/renderer/tsconfig.json --noEmit && node scripts/build.mjs && electron-builder",
    "tsc": "vue-tsc --project src/renderer/tsconfig.json --noEmit",
    "es:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue,.html",
    "es:lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.html",
    "style:fix": "stylelint . --fix .css,.less,.scss",
    "style:lint": "stylelint . --aei .css,.less,.scss",
    "format": "prettier --write ./**/*.{vue,ts,js,scss,json,md}",
    "test": "jest",
    "log": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-option.js",
    "prepare": "husky install"
  },
  "changelog": {
    "bugsUrl": "https://github.com/huoyou/vue_cli3/issues/",
    "emojis": true,
    "authorName": true,
    "authorEmail": true
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,.tsx,vue,html}": [
      "yarn es:fix"
    ],
    "./**/*.{css,less,scss}": [
      "yarn style:fix"
    ],
    "./**/*.{vue,ts,js,scss,json,md}": [
      "yarn format"
    ]
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "electron": "16.0.8",
    "electron-builder": "^22.14.5",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.0.4",
    "vite": "^5.0.12",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-resolve": "^2.5.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.4.4"
  },
  "dependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.3.2",
    "@vueuse/core": "^10.7.2",
    "amfe-flexible": "^2.2.1",
    "autoprefixer": "^10.4.2",
    "axios": "^1.6.7",
    "conventional-changelog-cli": "^4.1.0",
    "conventional-changelog-custom-config": "^0.3.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-vue": "^9.11.0",
    "jest": "^29.7.0",
    "naive-ui": "^2.25.2",
    "pinia": "^2.0.11",
    "pinia-plugin-persist": "^1.0.0",
    "postcss": "^8.4.6",
    "postcss-html": "^1.3.0",
    "postcss-pxtorem": "^6.0.0",
    "register-service-worker": "^1.7.2",
    "sass": "^1.49.7",
    "stylelint": "^16.2.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^4.5.0",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "ts-jest": "^29.1.2",
    "typescript-eslint": "^0.0.1-alpha.0",
    "vfonts": "^0.0.3",
    "vue-global-api": "^0.4.1",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-router": "^4.1.6"
  },
  "env": {
    "//": "Used in build scripts",
    "HOST": "127.0.0.1",
    "PORT": 3344
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ]
}
