{
  "name": "dashboard-editor",
  "version": "1.0.0",
  "main": "dist/main/index.cjs",
  "author": "Joel <xiao2731962667@163.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "build": "node scripts/build.mjs && electron-builder",
    "tsc-build": "vue-tsc --project src/renderer/tsconfig.json --noEmit && node scripts/build.mjs && electron-builder",
    "test": "jest",
    "fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue,.html",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.html",
    "style:lint": "stylelint . --fix .css,.less,.scss",
    "log": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-option.js",
    "prepare": "husky install",
    "tsc": "vue-tsc --project src/renderer/tsconfig.json --noEmit"
  },
  "changelog": {
    "bugsUrl": "https://github.com/huoyou/vue_cli3/issues/",
    "emojis": true,
    "authorName": true,
    "authorEmail": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config .commitlintrc.js"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,.tsx,vue,html,md}": "eslint --config .eslintrc.js",
    "*.{css,less,scss}": "stylelint --config .stylelintrc.js",
    "*.{ts,tsx,js,json,html,yml,css,less,md}": "prettier --write"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vitejs/plugin-vue-jsx": "^1.3.5",
    "electron": "16.0.8",
    "electron-builder": "^22.14.5",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-resolve": "^1.4.3",
    "vue": "^3.2.29",
    "vue-tsc": "^0.31.1"
  },
  "dependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "amfe-flexible": "^2.2.1",
    "autoprefixer": "^10.4.2",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-changelog-custom-config": "^0.3.1",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-vue": "^8.4.1",
    "jest": "^27.5.1",
    "naive-ui": "^2.25.2",
    "pinia": "^2.0.11",
    "pinia-plugin-persist": "^0.0.93",
    "postcss": "^8.4.6",
    "postcss-html": "^1.3.0",
    "postcss-pxtorem": "^6.0.0",
    "register-service-worker": "^1.7.2",
    "sass": "^1.49.7",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-jest": "^27.1.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "vue-global-api": "^0.4.1",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "env": {
    "//": "Used in build scripts",
    "HOST": "127.0.0.1",
    "PORT": 3344
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ]
}
