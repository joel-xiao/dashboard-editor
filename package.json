{
  "name": "dashboard-editor",
  "version": "1.0.0",
  "main": "dist/main/index.cjs",
  "author": "Joel <xiao2731962667@163.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "build": "node scripts/build.mjs && electron-builder",
    "tsc-build": "vue-tsc --project src/renderer/tsconfig.json --noEmit && node scripts/build.mjs && electron-builder",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "prepare": "husky install",
    "tsc": "vue-tsc --project src/renderer/tsconfig.json --noEmit"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "electron": "16.0.8",
    "electron-builder": "^22.14.5",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-resolve": "^1.4.3",
    "vue": "^3.2.29",
    "vue-tsc": "^0.31.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "naive-ui": "^2.25.2",
    "register-service-worker": "^1.7.2",
    "sass": "^1.49.7",
    "typescript-eslint": "^0.0.1-alpha.0",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "env": {
    "//": "Used in build scripts",
    "HOST": "127.0.0.1",
    "PORT": 3344
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ]
}
